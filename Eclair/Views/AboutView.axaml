<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="clr-namespace:Eclair.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Eclair.AboutView"
			 x:DataType="vm:ViewModel">
  <Grid>
	  <StackPanel HorizontalAlignment="Center"
				  Margin="14">
		  <Label Content="test"/>
	  </StackPanel>
	  <Separator/>
	  <StackPanel Orientation="Vertical"
				  VerticalAlignment="Bottom"
				  Margin="50">
		<Label Content="{Binding About_CreatedBy}"
			   HorizontalAlignment="Center"/>
		<Label Content="{Binding About_OriginalRepo}"
			   HorizontalAlignment="Center"/>
		<Label Content="https://github.com/NonExistPlayer/EclairPlayer"
			   HorizontalAlignment="Center"/>
		<Label HorizontalAlignment="Center">
			Copyright Â© 2024 Ilya Korobov</Label>
	  </StackPanel>
  </Grid>
</UserControl>
